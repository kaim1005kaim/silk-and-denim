{
  "compilerOptions": {
    "target": "ES2015",
    "module": "none",
    "lib": ["ES2015", "DOM"],
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@figma/plugin-typings"],
    "outFile": "code.js"
  },
  "include": ["code.ts"],
  "exclude": ["node_modules"]
}